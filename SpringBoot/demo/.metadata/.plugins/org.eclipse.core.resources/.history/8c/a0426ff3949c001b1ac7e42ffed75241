package com.example.demo.dtos;

import org.modelmapper.ModelMapper;

import lombok.Data;

@Data
public class alunoDTO {
	
	private int idAluno;
	private String nome;
	
	public alunoDTO toDto() {
		ModelMapper modelMapper = new ModelMapper();
		
		return modelMapper.map(this, alunoDTO.class);
	}
	
}
